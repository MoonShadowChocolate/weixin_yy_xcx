.loading-box1{
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: 999;
}
.loading-box2{
  position: absolute;
  left: 50%;
  top:50%;
  transform: translate(-50%,-50%);
  text-align: center;
}
.loading-img1{
  width: 100rpx;
  height: 100rpx;
}
.loading-box3{
  font-size: 32rpx;
  color: #666;
}